	function b1changeImage() {
 
    var image = document.getElementById('b1');
 
 
 
    if (image.src.match("static\img\b1sia")) {
 
        image.src = "static\img\b1.jpg";
 
    } else {
 
        image.src = "static\img\b1sia.jpg";
 
    }
 
 
 
   
 
}
 
 
 
